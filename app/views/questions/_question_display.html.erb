<% @questions.reverse.each do |q| %>
  <p><a href="/questions/<%= q.id %>/answers"><%= q.title %></a><span> <%= q.votes %></span></p>
  <p><%= q.content %></p>
  <%= button_to('Upvote', upvote_question_path(q), method: :put) %>
  <%= button_to('Downvote', downvote_question_path(q), method: :put) %>
  <%= button_to('Destroy', q, method: :delete) %>
  <%= button_to('Edit', edit_question_path(q), method: :get) %>
<% end %>
